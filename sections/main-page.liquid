<div class="container prose sm:prose-lg lg:prose-xl text-center">
  {% if page.title != blank %}
    <h1>{{ page.title }}</h1>
  {% endif %}

  {% if page.content != blank %}
    <p>{{ page.content }}</p>
  {% endif %}
</div>

{% schema %}
{
  "name": "t:sections.page.name",
  "tag": "section"
}
{% endschema %}